{
  // Required
  "manifest_version": 2,
  "name": "player Upipe",
  "version": "0.4",

  // Recommended
  "description": "A multimedia player based on the Upipe framework",

  // Optional
  "minimum_chrome_version": "35",
  "offline_enabled": true,
  "sockets": {
    "udp": {
      "send": "*",
      "bind": "*",
      "multicastMembership": "*"
    }
  },
  "permissions": [
    {
        "socket": [
            "udp-bind:*:*", 
            "udp-send-to:*:*"
        ]
    }
  ]
}
